 <table class="table table-striped table-hover table-condensed table-bordered items-table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Address</th>
        <th>Group</th>
        <th>Groupd Admin?</th>
      </tr>
    </thead>
    <tbody>
  <% @users.each do |x| %>
      <tr>
        <td><%= x.name %></td>
        <td><%= x.email %></td>
        <td><%= "#{x.house_number} #{x.street}" %></td>
        <td><%= x.group.name %></td>
        <td><%= x.group.admin == x ? "X" : "" %></td>
      </tr>
    <% end %>
    </tbody>
  </table>